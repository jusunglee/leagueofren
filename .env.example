# Duplicate this with your own values and rename to .env
# These can also be passed as CLI flags (e.g., --database-url)

# Database Configuration
DATABASE_URL=postgres://leagueofren:localdev123@localhost:5432/leagueofren?sslmode=disable

# Discord Configuration
DISCORD_TOKEN=your_discord_bot_token_here
# Set guild ID for instant command updates during development
# Leave empty for global registration (takes up to 1 hour to propagate)
GUILD_ID=your_test_server_id_here

# Riot API Configuration
RIOT_API_KEY=RGAPI-xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx

# LLM Configuration
LLM_PROVIDER=anthropic  # or "google"
LLM_MODEL=claude-sonnet-4-5-20250929
ANTHROPIC_API_KEY=your_anthropic_api_key_here
GOOGLE_API_KEY=your_google_api_key_here

# Optional Configuration (defaults shown)
# MAX_SUBSCRIPTIONS_PER_SERVER=10
# EVALUATE_SUBSCRIPTIONS_TIMEOUT=1m
# EVAL_EXPIRATION_DURATION=504h    # 3 weeks
# OFFLINE_ACTIVITY_THRESHOLD=168h  # 1 week
# NUM_CONSUMERS=2
